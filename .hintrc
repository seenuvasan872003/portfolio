{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "animation-timeline",
          "animation-timeline: view()",
          "animation-range",
          "animation-timeline: view(20% 80%)",
          "animation-timeline: view(50% 25%)",
          "animation-timeline: view(55% 25%)"
        ]
      }
    ]
  }
}